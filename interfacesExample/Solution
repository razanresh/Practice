public class Solution {
    private interface canFly {
        void fly();
    }
    private interface canSwim {
        void swim();
    }
    private interface canRun {
        void run();
    }
    class Duck{
        public interface canFly {
            void fly();
        }
        private interface canSwim {
            void swim();
        }
        private interface canRun {
            void run();
        }

    }
    class Penguin{
        private interface canSwim {
            void swim();
        }
        private interface canRun {
            void run();
        }
    }
    class Toad{
        public interface canSwim {
            void swim();
        }
    }
}
